<?xml version="1.0" encoding="utf-8" ?>
<Patch>	
	<Operation Class="PatchOperationFindMod">
				<mods>
					<li>Apothecary (Continued)</li>
				</mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationInsert">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildPlant"]/altitudeLayer</xpath>
				<value>
					<minifiedDef>MinifiedTree</minifiedDef>
				</value>
			</li>
			
			<li Class="PatchOperationInsert">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildPlant"]/plant</xpath>
				<value>
					<thingCategories>
						<li>Plants</li>
					</thingCategories>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildPlant"]/statBases</xpath>
				<value>
					<Mass>1</Mass>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildPlant"]/plant</xpath>
				<value>
					<treeLoversCareIfChopped>false</treeLoversCareIfChopped>
				</value>
			</li>

			<li Class="PatchOperationInsert">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildBush"]/altitudeLayer</xpath>
				<value>
					<minifiedDef>MinifiedTree</minifiedDef>
				</value>
			</li>
			
			<li Class="PatchOperationInsert">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildBush"]/plant</xpath>
				<value>
					<thingCategories>
						<li>Plants</li>
					</thingCategories>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildBush"]/statBases</xpath>
				<value>
					<Mass>1</Mass>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildBush"]/plant</xpath>
				<value>
					<treeLoversCareIfChopped>false</treeLoversCareIfChopped>
				</value>
			</li>	

			<li Class="PatchOperationInsert">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildTree"]/altitudeLayer</xpath>
				<value>
					<minifiedDef>MinifiedTree</minifiedDef>
				</value>
			</li>
			
			<li Class="PatchOperationInsert">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildTree"]/plant</xpath>
				<value>
					<thingCategories>
						<li>Plants</li>
					</thingCategories>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildTree"]/statBases</xpath>
				<value>
					<Mass>150</Mass>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="AYWildTree"]/plant</xpath>
				<value>
					<treeLoversCareIfChopped>true</treeLoversCareIfChopped>
				</value>
			</li>				
		</operations>
		</match>
	</Operation>
</Patch>