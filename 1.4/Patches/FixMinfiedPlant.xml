<?xml version="1.0" encoding="utf-8" ?>
<Patch>	
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationInsert">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="PlantBaseNonEdible"]/altitudeLayer</xpath>
				<value>
					<minifiedDef>MinifiedTree</minifiedDef>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="PlantBaseNonEdible"]/statBases</xpath>
				<value>
					<Mass>1</Mass>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="PlantBaseNonEdible"]/plant</xpath>
				<value>
					<treeLoversCareIfChopped>false</treeLoversCareIfChopped>
				</value>
			</li>
			
			<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[@Name="StumpBase"]/minifiedDef</xpath>
			</li>
			
			<li Class="PatchOperationInsert">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="StumpBase"]/altitudeLayer</xpath>
				<value>
					<minifiedDef>MinifiedTree</minifiedDef>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<success>Always</success>
				<xpath>Defs/ThingDef[@Name="StumpBase"]/statBases</xpath>
				<value>
					<Mass>1</Mass>
				</value>
			</li>
			
			<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Plant_SaguaroCactus"]/minifiedDef</xpath>
			</li>
			
			<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Plant_TreeCocoa"]/minifiedDef</xpath>
			</li>
			
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Royalty</li>
				</mods>
			<match Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Plant_TreeAnima"]/minifiedDef</xpath>
			</match>
			</li>
			
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Ideology</li>
				</mods>
			<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Plant_TreeGauranlen"]/minifiedDef</xpath>
				</li>
				
				<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Plant_TreeBonsai"]/minifiedDef</xpath>
				</li>
				
				<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Plant_Fibercorn"]/minifiedDef</xpath>
				</li>
				
				<li Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Plant_Timbershroom"]/minifiedDef</xpath>
				</li>
			</operations>
			</match>
			</li>
			
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Biotech</li>
				</mods>
			<match Class="PatchOperationRemove">
				<xpath>/Defs/ThingDef[defName="Plant_TreePolux"]/minifiedDef</xpath>
			</match>
			</li>
		</operations>
	</Operation>
</Patch>